<div class="headline">
  <h1>I Am The New Articles Template</h1>
  <h2>Create An Article</h2>
  <%=link_to "Home", root_path%>
  <%=form_for @article do |f|%>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <%end%>
</div>
